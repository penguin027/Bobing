.gotopage {
  position: fixed;
}

page {
  height: 100%;
}

.bgImg {
  width: 100%;
  height: 100%;
  position:fixed; 
  background-size:100% 100%;
  z-index: -1;
}
.tp {
  position: fixed;
  width: 90%;
  margin: 0 5%;
  bottom: 450rpx;
  left: 190rpx;
  display: flex;
  /* 如果把盒子设置为弹性盒子,必须要加的 */
  flex-direction: row;
  /* 设置一个大盒子内容垂直排列*/
  align-items: center;
  /* 设置一个大盒子内容居中排列*/
  justify-content: center;
  margin-top: 200rpx;
  width: 280rpx;
  height: 120rpx;
  color: var(--yellowColor);
  background-color: var(--redColor);
}

.other-widget {
  position: absolute;
}

.container {
  width: 100%;
  height: 1160rpx;
  margin: 0;
  padding: 0;
  position: relative;
}

image {
  position: absolute;
  width: 100%;
  height: 1160rpx;
}

.up {
  position: absolute;
  z-index: 1;
}